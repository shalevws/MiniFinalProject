<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ClientGUILoginController">
   <children>
      <Pane layoutY="6.0" prefHeight="343.0" prefWidth="484.0">
         <children>
            <Button fx:id="enter" layoutX="207.0" layoutY="218.0" mnemonicParsing="false" onAction="#getEnterBtn" text="Enter" />
            <Button fx:id="exit" layoutX="417.0" layoutY="300.0" mnemonicParsing="false" onAction="#getExitBtn" text="Exit" />
            <Label layoutX="44.0" layoutY="37.0" text="Email:" />
            <TextField fx:id="userName" layoutX="119.0" layoutY="30.0" prefHeight="31.0" prefWidth="207.0" />
            <Label layoutX="44.0" layoutY="75.0" text="Password:" />
            <TextField fx:id="password" layoutX="119.0" layoutY="68.0" prefHeight="31.0" prefWidth="207.0" />
            <RadioButton fx:id="radio_sub" layoutX="272.0" layoutY="178.0" mnemonicParsing="false" onAction="#getRadioSubBtn" selected="true" text="Subscriber" />
            <RadioButton fx:id="radio_lib" layoutX="96.0" layoutY="178.0" mnemonicParsing="false" onAction="#getRadioLibBtn" text="Librarian" />
            <Button fx:id="search" layoutX="189.0" layoutY="229.0" mnemonicParsing="false" onAction="#getSearchBtn" text="search For Book" />
         </children>
      </Pane>
   </children>
</AnchorPane>
